{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/BlinkLED.elf",
      "device": "STM32F407VG",
      "configFiles": [
        "/usr/share/openocd/scripts/interface/stlink.cfg",
        "/usr/share/openocd/scripts/target/stm32f4x.cfg"
      ],
      "gdbPath": "gdb-multiarch",
      "svdFile": "${workspaceRoot}/STM32F407.svd",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "both"
    }
  ]
}