{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/BlinkLED.elf",
      "device": "STM32F407VG",
      "showDevDebugOutput": "raw",
      "configFiles": [
        "/usr/share/openocd/scripts/interface/stlink.cfg",
        "/usr/share/openocd/scripts/target/stm32f4x.cfg"
      ],
      "gdbPath": "gdb-multiarch",
      "toolchainPrefix": "arm-none-eabi",
      "runToEntryPoint": "main",
      "preLaunchCommands": [
        "set mem inaccessible-by-default off",
        // Add path substitutions for your source files
        "set substitute-path /workspace/CubeMX ${workspaceRoot}",
        "dir ${workspaceRoot}"
      ]
    }
  ]
}